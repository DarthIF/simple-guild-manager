{
	"name": "simple-team-builder",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"smui-prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"devDependencies": {
		"@smui-extra/badge": "^8.0.3",
		"@smui/button": "^8.0.3",
		"@smui/card": "^8.0.3",
		"@smui/chips": "^8.0.3",
		"@smui/circular-progress": "^8.0.3",
		"@smui/data-table": "^8.0.3",
		"@smui/drawer": "^8.0.3",
		"@smui/fab": "^8.0.3",
		"@smui/icon-button": "^8.0.3",
		"@smui/layout-grid": "^8.0.3",
		"@smui/list": "^8.0.3",
		"@smui/ripple": "^8.0.3",
		"@smui/segmented-button": "^8.0.3",
		"@smui/snackbar": "^8.0.3",
		"@smui/tab": "^8.0.3",
		"@smui/tab-bar": "^8.0.3",
		"@smui/textfield": "^8.0.3",
		"@smui/tooltip": "^8.0.3",
		"@smui/top-app-bar": "^8.0.3",
		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/kit": "^2.36.2",
		"@sveltejs/vite-plugin-svelte": "^6.1.3",
		"@types/file-saver": "^2.0.7",
		"@zumer/snapdom": "^1.9.9",
		"file-saver": "^2.0.5",
		"smui-theme": "^8.0.3",
		"svelte": "^5.38.3",
		"svelte-check": "^4.3.1",
		"svelte-material-ui": "^8.0.3",
		"typescript": "^5.9.2",
		"vite": "^7.1.3"
	}
}